{% block title %}Edit Profile{% endblock %}

{% block body %}
    <h1>Edit Profile</h1>

    {{ form_start(form) }}
    {{ form_row(form.email) }}
    {{ form_row(form.firstName) }}
    {{ form_row(form.lastName) }}

    <button type="submit" class="btn btn-primary">Save Changes</button>
    {{ form_end(form) }}
{% endblock %}
